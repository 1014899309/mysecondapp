[super viewDidLoad];
     self.tabBarController.tabBar.hidden=YES;
    self.navigationItem.title = @"myFavor";
    self.imageArr = [[NSArray alloc]init];
    UITableView *tabView = [[UITableView alloc]initWithFrame:CGRectMake(0, 0, SCREEN_WIDTH, SCREEN_HEIGHT)];
    tabView.delegate = self;
    tabView.dataSource = self;
    [tabView registerNib:[UINib nibWithNibName:@"FirstViewCell" bundle:nil] forCellReuseIdentifier:@"cell"];
    tabView.tableFooterView = [[UIView alloc] init];
    [self.view addSubview:tabView];
    self.imageArr = [YQFmdb queryData:nil];
